create database teste;

use teste;

    create table alunos (
    id INT PRIMARY KEY,
    nome VARCHAR(100),
    idade INT
);
    insert into alunos (id, nome, idade)
values
(1, 'Lucas', 17),
(2, 'Ana', 19),
(3, 'Pedro', 18),
(4, 'Ana', 16);
    select * from alunos
    where idade >= 17 and idade <= 19
    order by nome asc ;
