create database teste;

use teste;

    create table alunos (
    id INT PRIMARY KEY,
    nome VARCHAR(100),
    idade INT
);
    insert into alunos (id, nome, idade)
values
(1, 'Lucas', 20),
(2, 'Ana', 19),
(3, 'Pedro', 18),
(4, 'Ana', 18),
(5, 'Pedrinho', 20);
    select idade, count(*)
    from alunos
    group by idade
    having count(*) > 1;
